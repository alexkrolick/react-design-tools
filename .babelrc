{
  "env": {
    "development": {
      "presets": [
        "env",
        "react-app/dev"
      ],
      "plugins": ["transform-flow-comments"]
    },
    "cjs": {
      "presets": [
        [
          "env",
          {
            "modules": "commonjs"
          }
        ],
        "react-app/prod"
      ],
      "plugins": ["transform-flow-comments"]
    },
    "es": {
      "presets": [
        [
          "env",
          {
            "modules": false,
            "loose": true
          }
        ],
        "react-app/prod"
      ],
      "plugins": ["transform-flow-comments"]
    },
    "test": {
      "presets": [
        [
          "env",
          {
            "modules": "commonjs",
          }
        ],
        "react-app/prod"
      ],
      "plugins": ["transform-flow-comments"]
    }
  }
}
